package ex04;

public class 로또02 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	int lotto[] = new int[6];
	for (int k=0; k<5; k++) {
	for (int i = 0; i < lotto.length; i++) {
		int num = (int)(Math.random()*45) + 1;
		lotto[i] = num;
		for(int j = 0; j < i; j++) {
			if(lotto[i] == lotto[j]) {
				i--;
				break;
		
			
			}
		}
	
		System.out.print(lotto[i] + " ");
	}
	}
	}
}
